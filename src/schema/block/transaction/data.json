{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/schema/block/transaction/data.json",

  "type": "object",
  "properties": {
    "seq": {
      "type": "integer",
      "minimum": 1
    },
    "cmd": {
      "type": "string",
      "pattern": "^data$"
    },
    "ns": {
      "type": "string",
      "pattern": "^[A-Za-z]{1,64}$"
    },
    "b64u": {
      "type": "string",
      "pattern": "^[A-Za-z0-9_-]{1,65536}$"
    }
  },
  "additionalProperties": false,
  "required": [
    "seq",
    "cmd",
    "ns",
    "b64u"
  ]
}