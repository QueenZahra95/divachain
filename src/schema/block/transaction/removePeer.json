{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/schema/block/transaction/removePeer.json",

  "type": "object",
  "properties": {
    "seq": {
      "type": "integer",
      "minimum": 1
    },
    "cmd": {
      "type": "string",
      "pattern": "^removePeer$"
    },
    "pk": {
      "type": "string",
      "pattern": "^[A-Za-z0-9_-]{43}$"
    }
  },
  "additionalProperties": false,
  "required": [
    "seq",
    "cmd",
    "pk"
  ]
}