{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "/schema/block/votes.json",

  "type": "object",
  "properties": {
    "origin": {
      "type": "string",
      "pattern": "^[a-f0-9]{64}$"
    },
    "sig": {
      "type": "string",
      "pattern": "^[a-f0-9]{128}$"
    }
  },
  "additionalProperties": false,
  "required": [
    "origin",
    "sig"
  ]
}